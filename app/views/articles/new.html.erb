<div class="container">
  <h1>New article</h1>
  <%= form_for(@article) do |f| %>
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title %></p>
    <p><%= f.label :content %></p>
    <p><%= f.text_field :content %></p>
    <p><%= f.submit %></p>
  <% end %>
  <%= link_to "back", articles_path %>
</div>
